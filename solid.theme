    fi

    # Prompt line #11

    if (( LP_ENABLE_DETACHED_SESSIONS )); then
        _lp_detached_sessions
        if [[ $lp_detached_sessions -gt 0 ]]; then
            color="$SP_COLOR_NOTE"
        else
            color="$SP_COLOR_BASE"
        fi
