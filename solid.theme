    fi


    # Prompt line #10
    if (( LP_ENABLE_JOBS )); then
        if [[ $lp_running_jobs -gt 0 ]]; then
            color="$SP_COLOR_NOTE"
        else
            color="$SP_COLOR_BASE"
        fi
